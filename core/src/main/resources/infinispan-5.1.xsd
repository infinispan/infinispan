        <xs:attribute name="hashFunctionClass" type="xs:string"/>
        <xs:attribute name="numOwners" type="xs:int"/>
        <xs:attribute name="numVirtualNodes" type="xs:int"/>
        <xs:attribute name="rehashEnabled" type="xs:boolean">
