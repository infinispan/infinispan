  </xs:complexType>

  <xs:complexType name="grouperConfiguration">
     <xs:attribute name="class" type="xs:string"/>
  </xs:complexType>

  <xs:complexType name="jmxStatistics">
    <xs:attribute name="enabled" type="xs:boolean"/>
  </xs:complexType>

  <xs:complexType name="storeAsBinary">
