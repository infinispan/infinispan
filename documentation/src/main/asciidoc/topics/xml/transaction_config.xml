<locking
   isolation="READ_COMMITTED"/>
<transaction
   locking="OPTIMISTIC"
   auto-commit="true"
   complete-timeout="1m"
   mode="NONE"
   notifications="true"
   reaper-interval="30s"
   recovery-cache="__recoveryInfoCacheName__"
   stop-timeout="30s"
   transaction-manager-lookup="org.infinispan.transaction.lookup.GenericTransactionManagerLookup"/>
