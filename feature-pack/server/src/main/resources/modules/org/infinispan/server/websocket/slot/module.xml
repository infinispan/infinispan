<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="urn:jboss:module:1.5" name="org.infinispan.server.websocket" slot="@infinispan.module.slot@">
   <properties>
      <property name="jboss.api" value="private" />
   </properties>

   <resources>
      <artifact name="${org.infinispan:infinispan-server-websocket}"/>
   </resources>

   <dependencies>
      <module name="javax.api" />
      <module name="javax.transaction.api" />
      <module name="org.apache.xerces" services="import" />
      <module name="org.infinispan.core" slot="@infinispan.module.slot@"/>
      <module name="org.infinispan.commons" slot="@infinispan.module.slot@"/>
      <!-- Netty is exported by server core -->
      <module name="org.infinispan.server" slot="@infinispan.module.slot@"/>
      <module name="org.jboss.logging" />
      <module name="org.jboss.marshalling" services="import" slot="@infinispan.module.slot@"/>
      <module name="org.jgroups" slot="@infinispan.module.slot@"/>
      <module name="com.fasterxml.jackson.core.jackson-core" slot="@infinispan.module.slot@"/>
      <module name="com.fasterxml.jackson.core.jackson-annotations" slot="@infinispan.module.slot@"/>
      <module name="com.fasterxml.jackson.core.jackson-databind" slot="@infinispan.module.slot@"/>
   </dependencies>
</module>
