<cache-container xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="urn:infinispan:config:16.1 https://infinispan.org/schemas/infinispan-config-fragment-16.1.xsd urn:infinispan:server:16.1 https://infinispan.org/schemas/infinispan-server-16.1.xsd"
                 xmlns="urn:infinispan:config:16.1"
                 name="default" statistics="true"
                 xmlns:server="urn:infinispan:server:16.1">
   <global-state unclean-shutdown-action="IGNORE" />
   <transport cluster="${infinispan.cluster.name:cluster}" stack="test-jdbc" server:data-source="mysql" />
   <serialization>
      <allow-list>
         <regex>.*</regex>
      </allow-list>
   </serialization>
   <metrics gauges="true" histograms="true" accurate-size="true" legacy="false" />
</cache-container>
