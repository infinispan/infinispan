        <subsystem xmlns="urn:infinispan:server:core:9.2" >
            <cache-container name="clustered" default-cache="default">
                <transport lock-timeout="240000"/>
                <distributed-cache
                        name="default"
                        segments="60"
                        owners="2"
                        batching="false"
                        l1-lifespan="0"
                        remote-timeout="30000" >
                    <locking
                            acquire-timeout="30000"
                            concurrency-level="1000"
                            striping="false" />
                    <state-transfer
                            enabled="true"
                            timeout="60000"
                            await-initial-transfer="false"/>
                </distributed-cache>
                <distributed-cache
                        name="memcachedCache"
                        segments="60"
                        owners="2"
                        batching="false"

                        l1-lifespan="0"
                        remote-timeout="30000" >
                    <locking
                            acquire-timeout="30000"
                            concurrency-level="1000"
                            striping="false" />
                    <state-transfer
                            enabled="true"
                            timeout="60000"
                            await-initial-transfer="false" />
                </distributed-cache>

                <distributed-cache
                        name="dist-state-transfer-async"
                        segments="60"
                        owners="2"
                        remote-timeout="30000" >
                    <state-transfer
                            enabled="true"
                            await-initial-transfer="false"/>
                </distributed-cache>
                <distributed-cache
                        name="dist-state-transfer-off"
                        segments="60"
                        owners="2"
                        remote-timeout="30000" >
                    <state-transfer
                            enabled="false"
                            await-initial-transfer="false"/>
                </distributed-cache>
                <replicated-cache
                        name="repl-state-transfer-async"
                        mode="SYNC"
                        remote-timeout="30000"
                        start="EAGER">
                    <state-transfer
                            enabled="true"
                            await-initial-transfer="false"/>
                </replicated-cache>
                <replicated-cache
                        name="repl-state-transfer-off"
                        mode="SYNC"
                        remote-timeout="30000"
                        start="EAGER">
                    <state-transfer
                            enabled="false"
                            await-initial-transfer="false"/>
                </replicated-cache>
            </cache-container>
        </subsystem>
