        <subsystem xmlns="urn:infinispan:server:core:${infinispan.core.schema.version}" >
            <cache-container
                name="clustered"
                default-cache="memcachedCache">
                <transport
                    lock-timeout="240000"/>
                <distributed-cache
                    name="memcachedCache"
                    owners="1"
                    l1-lifespan="20000">
                </distributed-cache>
            </cache-container>
        </subsystem>
