<endpoints socket-binding="default">
   <hotrod-connector name="hotrod">
      <authentication security-realm="default">
         <sasl mechanisms="SCRAM-SHA-512 SCRAM-SHA-384 SCRAM-SHA-256 SCRAM-SHA-1 DIGEST-SHA-512 DIGEST-SHA-384 DIGEST-SHA-256 DIGEST-SHA DIGEST-MD5 PLAIN" server-name="infinispan">
            <property name="com.sun.security.sasl.digest.realm">default</property>
         </sasl>
      </authentication>
   </hotrod-connector>
   <rest-connector name="rest"/>
</endpoints>
