<?xml version='1.0' encoding='UTF-8'?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>org.infinispan</groupId>
        <artifactId>infinispan-spring-parent</artifactId>
        <version>16.0.5-SNAPSHOT</version>
        <relativePath>../pom.xml</relativePath>
    </parent>

    <artifactId>infinispan-spring7-parent</artifactId>
    <name>Infinispan Spring 7 support</name>
    <packaging>pom</packaging>
    <properties>
        <infinispan.test.parallel.threads>1</infinispan.test.parallel.threads>
        <version.spring>${version.spring7}</version.spring>
        <version.spring.boot>${version.spring.boot4}</version.spring.boot>
        <version.spring.session>${version.spring.session4}</version.spring.session>
    </properties>
    <modules>
        <module>spring7-embedded</module>
        <module>spring7-remote</module>
    </modules>
</project>